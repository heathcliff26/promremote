{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "Auto merge all non-breaking changes",
      "matchManagers": ["*"],
      "matchDatasources": ["*"],
      "automerge": true,
      "automergeType": "branch",
      "matchUpdateTypes": ["minor", "patch", "digest"],
    },
  ]
}
